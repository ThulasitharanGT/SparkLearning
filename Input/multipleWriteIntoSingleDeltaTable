spark-submit --class org.controller.kafkaStreamExample.dataPullFromKafka --driver-memory 1g --driver-cores 1 --executor-memory 1g --executor-cores 1 --deploy-mode client --packages org.apache.kafka:kafka-clients:2.3.1,org.apache.kafka:kafka_2.11:2.3.1,org.apache.spark:spark-sql-kafka-0-10_2.11:2.4.4 /home/raptor/IdeaProjects/SparkLearning/build/libs parkLearning-1.0SNAPSHOT.jar keySerializer=org.apache.kafka.common.serialization.StringSerializer valueSerializer=org.apache.kafka.common.serialization.StringSerializer bootStrapServer=localhost:9092,localhost:9093,localhost:9094 topicName=carPerformanceMetricSensor checkPointLocationWrite=hdfs://localhost/user/raptor/kafka/stream/checkpointCarSensorDataFromStreamWrite/ checkPointLocationRead=hdfs://localhost/user/raptor/kafka/stream/checkpointCarSensorDataFromStreamRead/ outputPath=hdfs://localhost/user/raptor/kafka/stream/temp/consoleCarAndDriverSensorDataFromStreamDataLake/


